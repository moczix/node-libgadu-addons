{
  "name": "node-libgadu-js-ts",
  "version": "1.0.0",
  "description": "libgadu library for node javascript or typescript",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build",
	"test": "node test.js"
  },
  "binary": {
    "module_name": "gadu",
    "module_path": "./lib/binding/{node_abi}-{platform}-{arch}",
    "remote_path": "./{module_name}/v{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "host": ""
  },
  "keywords": [
    "libgadu",
    "typescript",
    "node",
    "gadugadu"
  ],
  "author": "moczniak",
  "email": "moczniak@gmail.com",
  "license": "ISC",
  "gypfile": true,
  "devDependencies": {
    "aws-sdk": "^2.7.7"
  },
  "dependencies": {
    "nan": "^2.4.0",
    "node-pre-gyp": "^0.6.31"
  },
  "repository": {
    "type": "git",
    "url": "https://moczniak@bitbucket.org/moczniak/node-libgadu-addons-js-ts.git"
  }
}
